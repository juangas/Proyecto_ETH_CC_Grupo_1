{
	"name": "front",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"lint": "eslint --fix . --ext js,jsx --max-warnings 0",
		"format": "prettier . --write",
		"preview": "vite preview"
	},
	"dependencies": {
		"bootstrap": "5.3.2",
		"ethers": "6.8.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router-dom": "6.20.0",
		"web3": "4.2.2"
	},
	"devDependencies": {
		"@types/react": "18.2.37",
		"@types/react-dom": "18.2.15",
		"@vitejs/plugin-react": "4.2.0",
		"eslint": "8.0.1",
		"eslint-config-prettier": "9.0.0",
		"eslint-config-standard": "17.1.0",
		"eslint-plugin-import": "2.25.2",
		"eslint-plugin-n": "15.0.0 || 16.0.0 ",
		"eslint-plugin-promise": "6.0.0",
		"eslint-plugin-react": "7.33.2",
		"husky": "8.0.3",
		"lint-staged": "15.1.0",
		"prettier": "3.1.0",
		"vite": "5.0.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"prettier --write .",
			"eslint --fix .",
			"git add"
		],
		"*.{html,css,less,ejs}": [
			"prettier --write .",
			"git add"
		]
	}
}
